{
    "name": "atvardovsky/laravel-openai-responses-core",
    "description": "Laravel package for working with OpenAI Responses API with minimal overhead",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "atvardovsky",
            "email": "atvardovsky@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/events": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Atvardovsky\\LaravelOpenAIResponses\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atvardovsky\\LaravelOpenAIResponses\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atvardovsky\\LaravelOpenAIResponses\\AIResponsesServiceProvider"
            ],
            "aliases": {
                "AIResponses": "Atvardovsky\\LaravelOpenAIResponses\\Facades\\AIResponses"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
